
<%= attachment_image_tag user, :profile_image, size:"63x63", fallback:"no_image.jpg" %>
<table class='table'>
  <tbody>
    <tr>
      <td>name</td>
      <td><%= user.name %></td>
    </tr>
    <tr>
      <td>introduction</td>
      <td><%= user.introduction %></td>
    </tr>
  </tbody>
</table>
<%= link_to edit_user_path(user), class:'btn btn-outline-secondary btn-block' do %>
    <i class="fas fa-user-cog"></i>
<% end %>
